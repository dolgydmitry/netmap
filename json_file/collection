                                    {
                                        "@name": "NONE",
                                        "ns0:description": {
                                            "ns0:text": "The client port will do nothing when a failure is\ndetected on the line port or the remote client port"
                                        }
                                    },
                                    {
                                        "@name": "LASER_SHUTDOWN",
                                        "ns0:description": {
                                            "ns0:text": "The client port will shut down the laser to notify the\nsubtending Ethernet equipment of the failure detected on\nthe line port or the remote client port."
                                        }
                                    },
                                    {
                                        "@name": "ETHERNET",
                                        "ns0:description": {
                                            "ns0:text": "The client port will propagate the local fault or remote\nfault signal to the subtending Ethernet equipment."
                                        }
                                    }
                                ]
                            },
                            "ns0:default": {
                                "@value": "ETHERNET"
                            },
                            "ns0:description": {
                                "ns0:text": "Sets the client port behavior that defines if the actions\nof automatic laser shutdown (als), ethernet fault\npropagation, or nothing will be done upon the detection\nof a failure on the line port or the upstream remote\nclient port."
                            }
                        },
