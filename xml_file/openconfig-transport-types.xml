  <typedef name="admin-state-type">
    <type name="enumeration">
      <enum name="ENABLED">
        <description>
          <text>Sets the channel admin state to enabled</text>
        </description>
      </enum>
      <enum name="DISABLED">
        <description>
          <text>Sets the channel admin state to disabled</text>
        </description>
      </enum>
      <enum name="MAINT">
        <description>
          <text>Sets the channel to maintenance / diagnostic mode</text>
        </description>
      </enum>
    </type>
    <description>
      <text>Administrative state modes for
logical channels in the transponder model.</text>
    </description>
  </typedef>
  <typedef name="loopback-mode-type">
    <type name="enumeration">
      <enum name="NONE">
        <description>
          <text>No loopback is applied</text>
        </description>
      </enum>
      <enum name="FACILITY">
        <description>
          <text>A loopback which directs traffic normally transmitted
on the port back to the device as if received on the same
port from an external source.</text>
        </description>
      </enum>
      <enum name="TERMINAL">
        <description>
          <text>A loopback which directs traffic received from an external
source on the port back out the transmit side of the same
port.</text>
        </description>
      </enum>
    </type>
    <default value="NONE"/>
    <description>
      <text>Loopback modes for transponder logical channels</text>
    </description>
  </typedef>
