  <grouping name="avg-min-max-instant-stats-precision2-ps-nm">
    <description>
      <text>Common grouping for recording picosecond per nanometer
values with 2 decimal precision. Values include the
instantaneous, average, minimum, and maximum statistics.
Statistics are computed and reported based on a moving time
interval (e.g., the last 30s).  If supported by the device,
the time interval over which the statistics are computed, and
the times at which the minimum and maximum values occurred,
are also reported.</text>
    </description>
    <leaf name="instant">
      <type name="decimal64">
        <fraction-digits value="2"/>
      </type>
      <units name="ps-nm"/>
      <description>
        <text>The instantaneous value of the statistic.</text>
      </description>
    </leaf>
    <leaf name="avg">
      <type name="decimal64">
        <fraction-digits value="2"/>
      </type>
      <units name="ps-nm"/>
      <description>
        <text>The arithmetic mean value of the statistic over the
time interval.</text>
      </description>
    </leaf>
    <leaf name="min">
      <type name="decimal64">
        <fraction-digits value="2"/>
      </type>
      <units name="ps-nm"/>
      <description>
        <text>The minimum value of the statistic over the time interval.</text>
      </description>
    </leaf>
    <leaf name="max">
      <type name="decimal64">
        <fraction-digits value="2"/>
      </type>
      <units name="ps-nm"/>
      <description>
        <text>The maximum value of the statistic over the time interval.</text>
      </description>
    </leaf>
    <uses name="oc-types:stat-interval-state"/>
    <uses name="oc-types:min-max-time"/>
  </grouping>
